  <xs:simpleType name="def_incomplete-date">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{4}(-[0-9]{2})?(-[0-9]{2})?"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- An IPI code -->
  <xs:simpleType name="def_ipi">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{11}"/>
    </xs:restriction>
  </xs:simpleType>
